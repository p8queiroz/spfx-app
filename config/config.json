{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": { 
    "app-home-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/appHome/AppHomeWebPart.js",
          "manifest": "./src/webparts/appHome/AppHomeWebPart.manifest.json"
        }
      ]
    },
    "app-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/app/AppWebPart.js",
          "manifest": "./src/webparts/app/AppWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "AppHomeWebPartStrings": "lib/webparts/appHome/loc/{locale}.js",
    "AppWebPartStrings": "lib/webparts/app/loc/{locale}.js"
  }
}
